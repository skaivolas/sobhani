                <form id="share-form" method="post" action="signaler_user.php">                    
                  <div id="share-inputs">                        
                    <label for="nom_expediteur">Ihr Name</label>                        
                    <input type="text" name="nom_expediteur" id="nom_expediteur"/>
                    <label for="from">Ihre E-Mail Adresse (*)</label>                        
                    <p class="error" id="error-from"></p>                        
                    <input type="text" name="from" id="from"/>
