function getCookieValue(c_name) {
  var c_start;
  var c_end;
  if (document.cookie.length>0) {
    c_start = document.cookie.indexOf(c_name + "=");
    if (c_start!=-1) {
      c_start = c_start + c_name.length+1;
      c_end = document.cookie.indexOf(";",c_start);
      if (c_end==-1) c_end = document.cookie.length;
      return unescape(document.cookie.substring(c_start,c_end));
    }
  }
  return "";
}
jQuery(document).ready(function($) {
    $(function() {
        var splitdomain = window.location.hostname.split('.');
if (splitdomain.length > 2) { splitdomain.shift(); }
        var currentdomain = splitdomain.join('.');
        var privacyPolicyUpd = new Date('2018-06-25T10:00:00'); 
        var privacyCookieValue=getCookieValue("__privacyalert");
        var acceptedPrivacyDate;
        if (privacyCookieValue!="") {
            var acceptedPrivacyDate = new Date(Date.parse(privacyCookieValue));
        }
        if (privacyCookieValue == "" || acceptedPrivacyDate < privacyPolicyUpd){
            var options = { year: 'numeric', month: 'long', day: 'numeric' };
            var displayedDate = privacyPolicyUpd.toLocaleDateString("de-DE", options);
            var language="de_DE";
    var langattr = language == "fr_FR"? "fr" : "en";
    var div = "<"+"div>";
    $(div).html("<span class=\"privacytext\">Diese Webseite verwendet Cookies und sammelt personenbezogene Daten.<br />F&uuml;r weitere Informationen lesen Sie bitte unsere <a href=\"https://www.openedition.org/19362?lang="+langattr+"\">Datenschutzerkl&auml;rung</a> (aktualisiert am "+displayedDate+").<br />Sie geben Einwilligung zu unseren Cookies, wenn Sie unsere Webseite weiterhin nutzen.</span><span class=\"closeprivacynotice\">schlie&szlig;en</span>").attr("id", "privacynotice").appendTo("body");
            $(".closeprivacynotice").click(function() {
                var expDate = new Date();
                expDate.setTime(expDate.getTime() + (365 * 24 * 3600 * 1000));
                var curDate = new Date();
                curDate.setTime(curDate.getTime());
                document.cookie = "__privacyalert="+curDate.toUTCString() +";expires=" + expDate.toUTCString() + ";domain="+currentdomain+";path=/";
                $("#privacynotice").remove();
            });
        }
    });
